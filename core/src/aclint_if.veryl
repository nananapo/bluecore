// #@@range_begin(mtime)
@<b>|import eei::*;|

interface aclint_if {
    var msip : logic ;
    var mtip : logic ;
    @<b>|var mtime: UInt64;|
    modport master {
        msip : output,
        mtip : output,
        @<b>|mtime: output,|
    }
// #@@range_end(mtime)
    modport slave {
        ..converse(master)
    }
}
